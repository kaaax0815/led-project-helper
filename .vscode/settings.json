{
  "files.exclude": {
    "node_modules/": true
  },
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.useTabStops": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": ["pinia"]
}
